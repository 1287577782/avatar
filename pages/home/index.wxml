<view class="home-page-header">
  <view class="swiper-wrap">
    <t-swiper
     wx:if="{{imgSrcs.length > 0}}"
      current="{{current}}"
      autoplay="{{autoplay}}"
      duration="{{duration}}"
      interval="{{interval}}"
      navigation="{{ navigation }}"
      imageProps="{{swiperImageProps}}"
      list="{{imgSrcs}}"
      bind:click="navToActivityDetail"
    >
    </t-swiper>
  </view>
  <t-grid class="block">
  <t-grid-item text="黄桥鹊桥" image="slot">
    <view slot="image" class="grid-item__image">
      <t-button variant="text" icon="share" open-type="share"></t-button>
    </view>
  </t-grid-item>
  <t-grid-item text="黄桥安居" icon="star" bindclick="onClick" />
  <t-grid-item text="黄桥招聘" icon="download" bindclick="onClick" />
  <t-grid-item text="黄桥家政" icon="edit-1" bindclick="onClick" />
  </t-grid>
</view>
<scroll-view>
<view class="home-page-body">
	<block wx:for="{{persionList}}" wx:for-index="index" wx:for-item="item" wx:key="{{item.id}}">
    <navigator class="container1" url="/pages/goods/goods?id={{item.id}}">
      <view class="card-header">
        <view><image class="img" src="{{item.img}}" mode="widthFix" ></image></view>
        <view><text>sdfsd</text></view>
        <!-- <view><text>dfhsjfh</text></view> -->
      </view>
      <view class="card-body">
        <view>
          <text>姓名</text><text>{{item.username}}</text>
          <text>性别</text> <text>未知</text>
          <text>职业</text>
        </view>
      </view>
    </navigator>
	</block>    
</view>
</scroll-view>
